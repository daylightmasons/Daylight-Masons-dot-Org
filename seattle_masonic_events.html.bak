<script language="javascript">
	function d(year, month, day) {
		return (new Date(year, month - 1, day));
	}
	
	function datePrint(d) {
		monthNames = new Array("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec")
		var curr_day = d.getDate();
		var curr_month = d.getMonth();
		var curr_year = d.getFullYear();
		
		return (monthNames[curr_month] + " " + curr_day + ", " + curr_year);
	}
	dateArray = new Array(d(2009,8,15),d(2009,8,26),d(2009,8,29),d(2009,09,13),d(2009,09,19),d(2009,09,23),d(2009,09,27),d(2009,10,3), d(2009,10,17),d(2009,10,31),d(2009,11,07),d(2009,11,21),d(2009,11,25),d(2009,12,05));
	eventArray = new Array("Stated Meeting and Degree Practice", "District #5 LOMA Meeting", "Entered Apprentice Degree Conferral", "ROOTS Breakfast <font style='color: red;'>red team</font>", "Stated Meeting", "District #5 LOMA Meeting", "ROOTS Breakfast <font style='color: blue;'>blue team</font>","Degree Conferral", "Stated Meeting", "District #5 LOMA Kid's Halloween Party and Dance", "Degree Conferral", "Stated Meeting - Grand Master Kenney To Visit", "District #5 LOMA Meeting", "Degree Conferral");
	if (dateArray.length != eventArray.length) {
		alert("Arrays do not match up");
	}
	even = true;
	for (key in dateArray) {
		var lessthan = (dateArray[key] < new Date());
		document.write('<tr class="' + ((even) ? 'evenMonth' : 'oddMonth') + '"><td class="borderRight">'+ ((lessthan) ? '<del>':'') + '<strong>' + datePrint(dateArray[key]) + '</strong>' + ((lessthan) ? '<del>' : '') +'</td><td class="padLeft">' + ((lessthan) ? '<del>':'') +  eventArray[key] + ((lessthan) ? '</del>':'') + '</td></tr>');
		even = !even;
	}
</script>
